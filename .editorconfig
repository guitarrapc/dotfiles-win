root = true

[*]
charset = utf-8
insert_final_newline = true
indent_style = space
trim_trailing_whitespace = true

[home/.docker/daemon.json]
indent_size = 2

[.gitattributes]
end_of_line = lf

[.profile]
end_of_line = lf

[.profile]
end_of_line = lf

[home/.config/git/*]
end_of_line = lf
indent_style = tab
insert_final_newline = true
tab_width = 4

[config]
end_of_line = lf

[*.conf]
end_of_line = lf
