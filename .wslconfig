[wsl2]
processors=8
memory=8GB
swap=0
# localhostForwarding=True # can't use with `networkingMode=mirrored`
nestedVirtualization=true
guiApplications=true
networkingMode=mirrored
dnsTunneling=true
autoProxy=true
[experimental]
# Docker Desktop Resource Saver and `gradual` cause WSL2 freeze: https://github.com/microsoft/WSL/issues/10901
# autoMemoryReclaim=gradual
autoMemoryReclaim=dropcache
